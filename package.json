{
    "name": "sfrpg",
    "version": "0.1.0",
    "description": "Starfinder game system definition for the Foundry Virtual Tabletop",
    "exports": {
        "import": "./index.js",
        "require": "./index.cjs"
    },
    "scripts": {
        "package": "gulp package",
        "build": "gulp build && gulp link",
        "buildDock": "gulp build && gulp copyUser",
        "build:watch": "gulp watch",
        "copyLocalization": "gulp copyLocalization",
        "cook": "gulp cook",
        "clean": "gulp clean && gulp link --clean",
        "cleanDock": "gulp clean && gulp copyUser --clean",
        "libs": "gulp libs",
        "update": "npm install --save-dev gitlab:foundry-projects/foundry-pc/foundry-pc-types",
        "unpack": "gulp unpack"
    },
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/foundryvtt-starfinder/foundryvtt-starfinder.git"
    },
    "keywords": [
        "starfinder",
        "system",
        "foundryvtt"
    ],
    "author": "The Foundry Starfinder Development Team",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/foundryvtt-starfinder/foundryvtt-starfinder/issues"
    },
    "homepage": "https://github.com/foundryvtt-starfinder/foundryvtt-starfinder#readme",
    "devDependencies": {
        "archiver": "^7.0.1",
        "chalk": "^5.3.0",
        "classic-level": "^1.4.1",
        "del": "^8.0.0",
        "eslint": "^9.12.0",
        "eslint-plugin-json": "^4.0.1",
        "fs-extra": "^11.2.0",
        "gulp": "^5.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-less": "^5.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-terser": "^2.1.0",
        "gulp-zip": "^6.0.0",
        "jquery": "^3.7.1",
        "jsdom": "^25.0.1",
        "json-stringify-pretty-compact": "^4.0.0",
        "path": "^0.12.7",
        "sanitize-filename": "^1.6.3",
        "yargs": "^17.7.2",
        "xlsx": "^0.18.5"
    },
    "dependencies": {}
}
